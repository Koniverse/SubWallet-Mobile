<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:editTextBackground">@drawable/rn_edit_text_material</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

    <style name="BootTheme" parent="Theme.BootSplash">
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="android:windowBackground" tools:targetApi="30">@drawable/splash_background</item>
        <item name="android:windowSplashScreenAnimatedIcon">@drawable/splash_logo</item>
        <item name="android:windowSplashScreenBackground">@color/splashscreen_bg</item>
    </style>
</resources>
